  "compilerOptions": {
    
    "target": "es5",                                
    "module": "commonjs",                           
    "outDir": "./build",                              
    "strict": true,
    "resolveJsonModule": true,                               
    "esModuleInterop": true,                        
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true        
  }
}
